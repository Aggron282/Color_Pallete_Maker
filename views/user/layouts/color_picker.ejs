<div class="color_picker_wrapper"></div>
  <% _id = pallete ? pallete.pallete_id :  "" %>
  <div class="color_picker_container" pallete_id = "<%=_id%>">

    <div class="picker_body">

        <p class="title"> Add Color </p>
        <p class="exit" id="picker_exit">X</p>
        <form id="picker_form">

          <div class="input_container">

            <div class="content_container">
              <div class="relative">
                <div class="add_color" id="add_picker">  + </div>
             </div>
              <div class="relative">
                <input class="picker_input" type="color" name="color_0" value = "#e6e6e6">
             </div>


            </div>




          </div>

          <button  class="picker_submit"> Submit Color </button>

          </form>

    </div>

  </div>
